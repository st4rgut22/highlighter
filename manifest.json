{
	"manifest_version":2,
	"name":"extension",
	"version":"0.1",
	"content_scripts":[
		{
			"matches": ["<all_urls>"],
			"js":["content.js","jquery-3.1.1.min.js"]
		}],
	"browser_action":{
				"default_icon":"icon.png"
			}
		,
	"background":{
		"scripts":["background.js"]
	},
	"permissions":[
		"storage",
		"tabs"
	]

}

